        </activity>
        <activity android:name="com.example.itsadmin.dottorhouse.MainActivity" >
        </activity>
        <activity android:name="com.example.itsadmin.dottorhouse.ProfiloMedico" >
        </activity>
        <activity android:name="com.example.itsadmin.dottorhouse.Settings" >
        </activity>

        <meta-data
            android:name="android.support.VERSION"
            android:value="25.4.0" />

        <provider
            android:name="com.android.tools.ir.server.InstantRunContentProvider"
            android:authorities="com.example.itsadmin.dottorhouse.com.android.tools.ir.server.InstantRunContentProvider"
            android:multiprocess="true" />
