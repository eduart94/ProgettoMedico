<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/item_ricerca"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">


    <TextView
        android:id="@+id/item_tipologia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/item_nome_medico"
        android:layout_marginTop="17dp"
        android:text="itemTipologia" />

    <TextView
        android:id="@+id/item_nome_medico"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/item_citta"
        android:layout_below="@+id/item_tipologia"
        android:layout_marginTop="17dp"
        android:text="itemNomeMedico" />

    <TextView
        android:id="@+id/item_cognome_medico"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/item_citta"
        android:layout_below="@+id/item_nome_medico"
        android:layout_marginTop="17dp"
        android:text="itemCognomeMedico" />


    <TextView
        android:id="@+id/item_citta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/item_cognome_medico"
        android:layout_marginBottom="17dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="17dp"
        android:text="itemCitta" />
</RelativeLayout>